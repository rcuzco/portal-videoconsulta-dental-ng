{
  "name": "portal-videoconsulta-dental-ng",
  "version": "1.0.2",
  "license": "MIT",
  "publishConfig": {
    "registry": "http://ic.sanitas.dom/nexus/repository/sanitas-npm-private/"
  },
  "scripts": {
    "ng": "ng",
    "start:hmr": "ng serve --hmr -e=hmr --open",
    "start": "ng serve --open",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "test": "npm run test:coverage",
    "test:coverage": "ng test --watch=false --code-coverage=true",
    "test:watch": "ng test --code-coverage=true",
    "lint": "ng lint && npm run lint:css",
    "lint:css": "stylelint \"src/**/*.scss\" --config .stylelintrc",
    "e2e": "npm run pree2e && ng e2e",
    "clean": "rimraf dist",
    "build:prod": "npm run clean && ng build --prod --vendor-chunk=true",
    "server:prod": "npm run build:prod && http-server dist/app -c-1 --cors",
    "server:prod:jit": "ng build --prod --aot=false --build-optimizer=false && http-server dist/app -c-1 --cors",
    "documentation": "./node_modules/.bin/compodoc -p tsconfig.json -d documentation/ -s -o ",
    "mocks": "node ./mock_server/index.js",
    "checkQA": "npm run lint && npm run test && npm run build:prod",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "pre-push": [
    "build",
    "test"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run build:prod && npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@angular/animations": "9.0.6",
    "@angular/common": "9.0.6",
    "@angular/compiler": "9.0.6",
    "@angular/core": "9.0.6",
    "@angular/forms": "9.0.6",
    "@angular/platform-browser": "9.0.6",
    "@angular/platform-browser-dynamic": "9.0.6",
    "@angular/router": "9.0.6",
    "@angularclass/hmr": "2.1.3",
    "@angularclass/hmr-loader": "3.0.4",
    "@ngx-translate/core": "12.1.2",
    "@ngx-translate/http-loader": "4.0.0",
    "@opentok/client": "2.17.0",
    "core-js": "3.6.4",
    "rxjs": "6.5.4",
    "tslib": "1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.5",
    "@angular/cli": "9.0.5",
    "@angular/compiler-cli": "9.0.6",
    "@angular/language-service": "9.0.6",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-angular": "8.3.4",
    "@types/jasmine": "3.5.5",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.11.1",
    "codelyzer": "5.2.1",
    "exports-loader": "0.7.0",
    "http-server": "0.11.1",
    "husky": "4.2.2",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage": "1.1.2",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-remap-coverage": "0.1.5",
    "karma-sourcemap-loader": "0.3.7",
    "path": "0.12.7",
    "postcss-custom-properties": "9.1.1",
    "postcss-loader": "3.0.0",
    "postcss-url": "8.0.0",
    "protractor": "5.4.3",
    "rimraf": "2.6.3",
    "serve-favicon": "2.5.0",
    "stylelint": "13.2.0",
    "ts-node": "7.0.1",
    "tslint": "6.0.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.7.5"
  }
}
